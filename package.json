{
  "name": "millez-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/millez-components/millez-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/millez-components/millez-components.esm.js",
      "require": "./dist/millez-components/millez-components.cjs.js"
    },
    "./mlz-avatar": {
      "import": "./dist/components/mlz-avatar.js",
      "types": "./dist/components/mlz-avatar.d.ts"
    },
    "./mlz-image-selector": {
      "import": "./dist/components/mlz-image-selector.js",
      "types": "./dist/components/mlz-image-selector.d.ts"
    },
    "./mlz-code-validation": {
      "import": "./dist/components/mlz-code-validation.js",
      "types": "./dist/components/mlz-code-validation.d.ts"
    },
    "./mlz-spinner": {
      "import": "./dist/components/mlz-spinner.js",
      "types": "./dist/components/mlz-spinner.d.ts"
    },
    "./mlz-loading-button": {
      "import": "./dist/components/mlz-loading-button.js",
      "types": "./dist/components/mlz-loading-button.d.ts"
    },
    "./mlz-color-selector": {
      "import": "./dist/components/mlz-color-selector.js",
      "types": "./dist/components/mlz-color-selector.d.ts"
    },
    "./mlz-range": {
      "import": "./dist/components/mlz-range.js",
      "types": "./dist/components/mlz-range.d.ts"
    },
    "./mlz-create-password": {
      "import": "./dist/components/mlz-create-password.js",
      "types": "./dist/components/mlz-create-password.d.ts"
    },
    "./mlz-input": {
      "import": "./dist/components/mlz-input.js",
      "types": "./dist/components/mlz-input.d.ts"
    },
    "./mlz-image-cropper": {
      "import": "./dist/components/mlz-image-cropper.js",
      "types": "./dist/components/mlz-image-cropper.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@stencil/sass": "^3.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "@pqina/pintura": "^8.92.19",
    "tui-color-picker": "^2.2.8",
    "tui-image-editor": "^3.15.3"
  }
}
